# ==== UART1 ====
Net "uart_txd1" LOC = "G11" | IOSTANDARD = "LVCMOS33";
Net "uart_rxd1" LOC = "F10" | IOSTANDARD = "LVCMOS33";


# ==== SPI ====
NET "miso"	LOC = "T12"	| IOSTANDARD = "LVCMOS33";		
